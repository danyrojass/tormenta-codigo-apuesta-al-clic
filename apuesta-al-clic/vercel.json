{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ganador",
      "dest": "dist/server.js"
    },
    {
      "src": "/api/ganador/reset",
      "dest": "dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/client/$1"
    }
  ]
}
